Cell[TextData[{
 "Denoting the position of star 1 as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["r", "\[Rule]"], "1"], "=", 
    RowBox[{"{", " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["x", "1"], "[", "t", "]"}], ",", 
      RowBox[{
       SubscriptBox["y", "1"], "[", "t", "]"}], ",", 
      RowBox[{
       SubscriptBox["z", "1"], "[", "t", "]"}]}], " ", "}"}]}], 
   TraditionalForm]],ExpressionUUID->"811c3ac8-3e90-4150-83bd-75bad4bce091"],
 " and the position of star 2 as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["r", "\[Rule]"], "2"], "=", 
    RowBox[{"{", " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["x", "2"], "[", "t", "]"}], ",", 
      RowBox[{
       SubscriptBox["y", "2"], "[", "t", "]"}], ",", 
      RowBox[{
       SubscriptBox["z", "2"], "[", "t", "]"}]}], " ", "}"}]}], 
   TraditionalForm]],ExpressionUUID->"1b466afd-9c1d-4c38-b844-ea37bb33776b"],
 ", the force on star 1 due to star 2, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["F", "\[Rule]"], "12"], TraditionalForm]],ExpressionUUID->
  "b03a7260-c504-4c49-bbc0-5211f662ec79"],
 ", is then given by the following:\[LineSeparator]\[LineSeparator]    \
\[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["F", "\[Rule]"], "12"], "   ", ":=", "   ", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         RowBox[{"G", " ", 
          SubscriptBox["m", "1"], " ", 
          SubscriptBox["m", "2"]}], 
         SuperscriptBox["r", "2"]]}], 
       SubscriptBox[
        OverscriptBox["r", "^"], "12"]}], "  ", ":=", "   ", 
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         RowBox[{"G", " ", 
          SubscriptBox["m", "1"], " ", 
          SubscriptBox["m", "2"]}], 
         SuperscriptBox["r", "3"]]}], 
       SubscriptBox[
        OverscriptBox["r", "\[Rule]"], "12"]}]}]}], " ", ","}], 
   TraditionalForm]],ExpressionUUID->"d9848b23-0004-474c-8ea2-ae873a4823cb"],
 "\[LineSeparator]\[LineSeparator]    \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["r", "\[Rule]"], "12"], "   ", ":=", "   ", 
     RowBox[{
      RowBox[{
       RowBox[{"r", "  ", 
        SubscriptBox[
         OverscriptBox["r", "^"], "12"], "   ", "with"}], " ", "|", 
       SubscriptBox[
        OverscriptBox["r", "^"], "12"], "|"}], "=", "1"}]}], " ", ","}], 
   TraditionalForm]],ExpressionUUID->"c00a8516-cfba-4f30-9854-5181a60b6531"],
 "\[LineSeparator]\[LineSeparator]    \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["r", "\[Rule]"], "12"], "   ", ":=", "   ", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["r", "\[Rule]"], "1"], "-", 
      SubscriptBox[
       OverscriptBox["r", "\[Rule]"], "2"]}]}], " ", ","}], TraditionalForm]],
  ExpressionUUID->"df735cc8-fd71-4298-96dc-7b935f4c300e"],
 "\[LineSeparator]\[LineSeparator]    \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "        ", ":=", "  ", 
    RowBox[{
     SqrtBox[
      SuperscriptBox[
       SubscriptBox[
        OverscriptBox["r", "\[Rule]"], "12"], "2"]], "                 ", "=",
