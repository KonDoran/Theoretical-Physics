       RowBox[{"d", " ", "x"}]]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["x", "2"], "-", 
        SuperscriptBox["n", "2"]}], ")"}], "y"}]}], "==", "0"}], 
   TraditionalForm]],ExpressionUUID->"7eafda27-1138-4a5e-876e-40c1ae9099eb"],
