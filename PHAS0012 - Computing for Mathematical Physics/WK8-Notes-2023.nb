Cell[TextData[{
 "E.g., in the case of star 1, the equation of motion is being passed to ",
 StyleBox["NDSolve[...]",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " in the form\n\[LineSeparator]    \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["x", "1"], "''"}], "[", "t", "]"}]}], ",", " ", 
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["y", "1"], "''"}], "[", "t", "]"}]}], ",", " ", 
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["z", "1"], "''"}], "[", "t", "]"}]}]}], "}"}], "==", 
     " ", 
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["F", 
        RowBox[{"12", ",", "x"}]], ",", " ", 
       SubscriptBox["F", 
        RowBox[{"12", ",", "y"}]], ",", " ", 
       SubscriptBox["F", 
        RowBox[{"12", ",", "z"}]]}], "}"}]}], " ", ","}], TraditionalForm]],
  ExpressionUUID->"ed2c778b-154c-48f3-9b88-b65e44f62fe0"],
 "\n\[LineSeparator]when ",
 StyleBox["NDSolve[...]",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " instead expects a list with a ",
 StyleBox["separate element for each vector",
  FontSlant->"Italic"],
 " ",
 StyleBox["component",
  FontSlant->"Italic"],
 " in that equation of motion,  \n\[LineSeparator]    \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["x", "1"], "''"}], "[", "t", "]"}]}], "==", 
       SubscriptBox["F", 
        RowBox[{"12", ",", "x"}]]}], ",", "  ", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["y", "1"], "''"}], "[", "t", "]"}]}], "==", 
       SubscriptBox["F", 
        RowBox[{"12", ",", "y"}]]}], ",", " ", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", "1"], " ", 
        RowBox[{
         RowBox[{
          SubscriptBox["z", "1"], "''"}], "[", "t", "]"}]}], "==", 
       SubscriptBox["F", 
        RowBox[{"12", ",", "z"}]]}], ",", " ", " ", 
      StyleBox["etc",
       FontSlant->"Italic"]}], "}"}], " ", "."}], TraditionalForm]],
  ExpressionUUID->"33776599-4e5d-4832-8898-3395929c99ad"],
 "\[LineSeparator]"
}], "Item1",
