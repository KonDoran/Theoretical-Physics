Cell[TextData[{
 "With the ",
 StyleBox["guess[...]",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " function in hand one can go on to use it to quickly make solutions of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["d", "2"], "y"}], 
      RowBox[{"d", " ", 
       SuperscriptBox["x", "2"]}]], "-", 
     RowBox[{"k", 
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"d", " ", "y"}], 
           RowBox[{"d", " ", "x"}]], ")"}], "2"]}]]}]}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"ea8fad8c-21ee-42b1-908c-f1d1d5110d6c"],
 ", repeatedly, until one arrives at a solution which has the far end of the \
chain where it should be ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"y", "[", 
      RowBox[{"x", "=", 
       RowBox[{"d", "=", "10"}]}], "]"}], "=", "0"}]}], TraditionalForm]],
  ExpressionUUID->"7234a35e-38cb-41da-9cea-4f2c60921638"],
 ").\[LineSeparator]"
}], "Item1",
