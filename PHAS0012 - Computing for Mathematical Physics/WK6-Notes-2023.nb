Cell["Ordinary Differential Equations", "Section",ExpressionUUID->"14e8e998-a03f-4e6c-be1d-9eeb9bc0b7d7"],

Cell[CellGroupData[{

Cell["\<\
Ordinary differential equations, especially if they are nonlinear, may be \
hard or impossible to solve analytically.\
\>", "Item1",
 CellChangeTimes->{{3.664695698692412*^9, 3.664695710108163*^9}, {
  3.885206319402183*^9, 3.885206319484659*^9}, {3.8852066921571693`*^9, 
  3.8852067043855047`*^9}},ExpressionUUID->"672e9d46-3618-463f-8885-\
c982e3cb1ee3"],

Cell["Here is an example.", "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.664695698692412*^9, 3.664695710108163*^9}, {
   3.885206319402183*^9, 3.885206325824291*^9}, 
   3.885206460366419*^9},ExpressionUUID->"b3410c34-20f4-4df5-b17a-\
91baf0f63ac5"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Attempt", " ", "to", " ", "get", " ", "an", " ", "analytic", " ", 
    "solution", " ", "with", " ", 
    RowBox[{"DSolve", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Differential", " ", 
       RowBox[{"equation", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"y", "''"}], "[", "x", "]"}], "+", 
        FractionBox["x", 
         RowBox[{"y", "[", "x", "]"}]]}], "==", "0"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Boundary", " ", 
        RowBox[{"conditions", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"y", "'"}], "[", "0", "]"}], "==", "1"}], ",", 
      RowBox[{
       RowBox[{"y", "[", "0", "]"}], "\[Equal]", 
       FractionBox["1", "2"]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Function", " ", "to", " ", "solve", " ", 
      RowBox[{"for", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"y", "[", "x", "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Independent", " ", 
      RowBox[{"variable", "."}]}], " ", "*)"}], "\[IndentingNewLine]", "x"}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.884845693029943*^9, 3.884845698731573*^9}, {
  3.885206336298669*^9, 3.885206453269144*^9}, {3.88520656294881*^9, 
  3.885206563442891*^9}, {3.8852104780496483`*^9, 3.885210480781817*^9}},
 CellLabel->
  "2/12/23 17:54:00 \
In[1]:=",ExpressionUUID->"97631334-33b4-4a42-858a-116bc68ade87"],

Cell[TextData[{
 "To seek a numerical solution instead, one needs to use ",
 StyleBox["NDSolve",
  FontFamily->"Courier New"],
 " rather than ",
 StyleBox["DSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 "."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.664695761733221*^9, 3.664695764765561*^9}, {
  3.885206475429551*^9, 3.885206509334753*^9}, {3.885206600926445*^9, 
  3.8852066045623198`*^9}, {3.885206726883958*^9, 
  3.885206750855122*^9}},ExpressionUUID->"ed59d598-5759-4d7c-a613-\
0b11d74e9765"],

Cell[TextData[{
 "When doing so, one needs to declare not only the independent variable as \
the final argument \[LongDash] in this case ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3003938e-b4c6-4889-b725-8794c6269402"],
 " \[LongDash] but also the associated finite domain that one requires a \
solution for; below ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", 
    FractionBox["3", "2"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b191ec58-f896-4c3c-9a51-58763e60064a"],
 "."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.664695761733221*^9, 3.664695764765561*^9}, {
  3.885206475429551*^9, 3.885206509334753*^9}, {3.885206600926445*^9, 
  3.885206683282133*^9}, {3.885206777301813*^9, 3.8852068431894503`*^9}, {
  3.885211093586281*^9, 3.885211094124837*^9}, {3.88521227857032*^9, 
  3.885212279402927*^9}},ExpressionUUID->"b1b644c0-5e02-4982-9461-\
2ee5bf6313ec"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Attempt", " ", "to", " ", "get", " ", "a", " ", "numerical", " ", 
    "solution", " ", "with", " ", 
    RowBox[{"NDSolve", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"numericalSolutionReplacementRule", "=", "\[IndentingNewLine]", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Differential", " ", 
        RowBox[{"equation", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"y", "''"}], "[", "x", "]"}], "+", 
         FractionBox["x", 
          RowBox[{"y", "[", "x", "]"}]]}], "==", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Boundary", " ", 
         RowBox[{"conditions", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"y", "'"}], "[", "0", "]"}], "==", "1"}], ",", 
       RowBox[{
        RowBox[{"y", "[", "0", "]"}], "\[Equal]", 
        FractionBox["1", "2"]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Function", " ", "to", " ", "solve", " ", 
       RowBox[{"for", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"y", "[", "x", "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Independent", " ", "variable", " ", "AND", " ", "the", " ", 
       "associated", " ", "range"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "for", " ", "which", " ", "the", " ", "solution", " ", "should", " ", 
       "be", " ", 
       RowBox[{"valid", "."}]}], "       ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", 
       FractionBox["3", "2"]}], "}"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]], "Input",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.602243030810259*^9, 3.6022430309058104`*^9}, {
   3.884845701159157*^9, 3.884845708044559*^9}, {3.885206410678239*^9, 
   3.885206428465247*^9}, {3.885206464148368*^9, 3.885206470603487*^9}, {
   3.8852065153416224`*^9, 3.885206587550494*^9}, {3.8852071204838*^9, 
   3.885207152489156*^9}, {3.885207916233741*^9, 3.885207920325869*^9}, 
   3.885817037941824*^9},ExpressionUUID->"26c228b5-c9b4-4979-97b1-\
4ebf481bfbd4"],

Cell[TextData[{
 "Note the form in which the solution is returned: it is basically a series \
of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "33bbcc64-34f2-4e8d-b6ca-8b4dd016b243"],
 "-value pairs, with any asked-for in-between values returned via \
interpolation, a so-called ",
 StyleBox["interpolating function",
  FontSlant->"Italic"],
 ". "
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 3.885206880453224*^9}, {
  3.8852111073197947`*^9, 3.88521110836694*^9}, {3.8852123390920753`*^9, 
  3.885212427925392*^9}, {3.885212555132894*^9, 3.885212575943857*^9}, {
  3.885212668019458*^9, 
  3.885212694016127*^9}},ExpressionUUID->"96182d32-a83f-45ee-8fda-\
58b39845e87d"],

Cell[TextData[{
 "The operation of ",
 StyleBox["NDSolve",
  FontFamily->"Courier New"],
 " is very sophisticated."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 
  3.885206886558507*^9}},ExpressionUUID->"e5fba06f-9a53-4c3b-9a80-\
aba5cbec952a"],

Cell[TextData[{
 "In particular, when computing interpolating function solutions ",
 StyleBox["NDSolve",
  FontFamily->"Courier New"],
 " adjusts the number and spacing of the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3db10288-00e5-4400-b1a5-c56a5f796720"],
 " values such that regions where the function changes rapidly with ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3f2061d0-6511-4b3e-ad1f-4f41a26ed159"],
 " have a higher density of points than regions where it is varying slowly."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 3.885207000086856*^9}, {
  3.8852111144866457`*^9, 3.885211145995266*^9}, {3.885212479777574*^9, 
  3.8852124797795*^9}, {3.885212621808386*^9, 
  3.885212650582676*^9}},ExpressionUUID->"eeb02604-1a08-46cd-8b61-\
a18f47d720fe"],

Cell[TextData[{
 "This leads to ",
 StyleBox["far",
  FontSlant->"Italic"],
 " more accurate numerical solutions than would be obtained using an \
interpolating function seeded by evenly spaced ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7ad226a4-ddb6-44f8-87d9-44ae2270b7b1"],
 "-points."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 3.885207000086856*^9}, {
  3.8852111144866457`*^9, 3.885211145995266*^9}, {3.885212480291725*^9, 
  3.885212541251481*^9}},ExpressionUUID->"49be5174-31ab-465b-bbed-\
acafeb5289cf"],

Cell[TextData[{
 "The only way one can validate the solutions returned by ",
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " is to substitute them back into the LHS of the differential equation from \
which they were derived, and check for consistency by plotting the result.  "
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 3.885207058780171*^9}, {
  3.885211167165929*^9, 3.885211229604846*^9}, {3.885212754357284*^9, 
  3.885212764267879*^9}},ExpressionUUID->"76e9fb14-3f4b-47b4-be92-\
3b9b49ebb104"],

Cell[TextData[{
 "For the case in hand here, this implies that we insert the interpolating \
function solutions output by ",
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " into ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["d", "2"], "y"}], 
     RowBox[{"d", " ", 
      SuperscriptBox["x", "2"]}]], "+", 
    FractionBox["x", "y"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5b312d89-a713-411f-99cb-035a226561b3"],
 ", and check whether the latter quantity is indeed equal to zero throughout \
the solved-for range in ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5f45f99c-a867-49ca-8dba-461e48d79a2f"],
 "."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.885206848566905*^9, 3.885207058780171*^9}, {
  3.885211167165929*^9, 3.885211327141795*^9}, {3.885212802425489*^9, 
  3.885212822404315*^9}},ExpressionUUID->"84ca6820-a5cc-472d-86a7-\
641089440151"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"numericalSolution", "[", "x_", "]"}], "=", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"y", "[", "x", "]"}], "/.", 
    "numericalSolutionReplacementRule"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"numericalSolution", "''"}], "[", "x", "]"}], "+", 
    FractionBox["x", 
     RowBox[{"numericalSolution", "[", "x", "]"}]]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", 
     FractionBox["1", "5"]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotRange", "\[Rule]", "All"}]}], "\[IndentingNewLine]", 
  "]"}]}], "Input",
