7e043436bb4c"],

Cell[TextData[{
 "For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "[", 
    RowBox[{"x", "+", 
     RowBox[{"i", " ", "\[Delta]x"}]}], "]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9cf4bf5f-afce-476a-b6ed-48e99ce238d3"],
 " we have, analogously:\[LineSeparator]  \[FilledSmallCircle]    ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "[", 
     RowBox[{"x", "+", 
      RowBox[{"i", " ", "\[Delta]x"}]}], "]"}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", "\[Delta]x"}], ")"}], 
      RowBox[{"y", "[", 
       RowBox[{"x", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", "-", "1"}], ")"}], "\[Delta]x"}]}], "]"}]}], "=", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", "\[Delta]x"}], ")"}], "i"], 
      RowBox[{
       RowBox[{"y", "[", "x", "]"}], " ", "."}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ae3aa394-b992-456c-9f07-b4857069e708"]
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.665162112024867*^9, 3.665162113768833*^9}, {
  3.885217618434545*^9, 3.885217628607265*^9}, {3.88521975847714*^9, 
