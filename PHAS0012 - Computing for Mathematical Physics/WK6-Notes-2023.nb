Cell[TextData[{
 "Now ",
 StyleBox["FindMinimum[...]",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " locates a minimum in a completely different place, ",
 StyleBox["{r12\[Rule]2.77259, r23\[Rule]2.31049, r13\[Rule]100.}",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 ", with the value of the ",
 StyleBox["totalEnergy[...]",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " function now being ",
 StyleBox["-0.25",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 ", whereas previously, at the other location, it reported a minimum total \
energy of ",
 StyleBox["-0.375",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 ".  "
}], "Item1",
