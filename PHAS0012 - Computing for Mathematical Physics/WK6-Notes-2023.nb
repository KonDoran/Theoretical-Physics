Cell["\<\
In this appendix we quickly present one final variation on the finite \
difference approaches to the problem considered at the end of the final \
section above: the so-called Lax-Wendroff scheme.\[LineSeparator]\
\>", "Item1",
 CellChangeTimes->{{3.727728146879181*^9, 3.727728169510229*^9}, {
  3.885295169880269*^9, 3.885295173969059*^9}, {3.88538028847358*^9, 
  3.885380424562202*^9}, {3.8854507752358522`*^9, 3.885450817291069*^9}, {
  3.885450908438712*^9, 3.885450944282981*^9}, {3.885457592012458*^9, 
  3.885457604493174*^9}, {3.885457919972766*^9, 3.885457936330367*^9}, {
  3.8858171426690474`*^9, 
  3.885817191580099*^9}},ExpressionUUID->"3de285f3-e477-4035-b30f-\
44d70cfd8a99"],

Cell[TextData[{
 "This scheme starts with the following standard Taylor expansion of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "[", 
    RowBox[{"x", ",", 
     RowBox[{"t", "+", 
      RowBox[{"\[Delta]", " ", "t"}]}]}], "]"}], TraditionalForm]],
  ExpressionUUID->"e32eb0fe-19f3-4c7c-bb25-902e48a5c4b4"],
 " about ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Delta]", " ", "t"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"d13f4822-c2c0-4dae-bfbc-cdfbb731e548"],
 ":\[LineSeparator]  \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "[", 
     RowBox[{"x", ",", 
      RowBox[{"t", "+", 
       RowBox[{"\[Delta]", " ", "t"}]}]}], "]"}], "  ", "=", " ", 
    RowBox[{
     RowBox[{"y", "[", 
      RowBox[{"x", ",", "t"}], "]"}], " ", "+", " ", 
     RowBox[{"\[Delta]", " ", "t", " ", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", " ", "t"}]], 
       RowBox[{"y", "[", 
        RowBox[{"x", ",", "t"}], "]"}]}]}], " ", "+", " ", 
     RowBox[{
      FractionBox["1", "2"], "\[Delta]", " ", 
      SuperscriptBox["t", "2"], " ", 
      RowBox[{
       FractionBox[
        SuperscriptBox["\[PartialD]", "2"], 
        RowBox[{"\[PartialD]", " ", 
         SuperscriptBox["t", "2"]}]], 
       RowBox[{"y", "[", 
        RowBox[{"x", ",", "t"}], "]"}]}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{"O", "(", 
       RowBox[{"\[Delta]", " ", 
        SuperscriptBox["t", "3"]}], ")"}], " ", "."}]}]}], TraditionalForm]],
  CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
  CellChangeTimes->{{3.8853758269198933`*^9, 3.88537583220271*^9}},
  ExpressionUUID->"d56a1fc3-8050-4cc6-9c8b-4138a30e07b0"],
 "\[LineSeparator]"
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.727728146879181*^9, 3.727728169510229*^9}, {
   3.885295169880269*^9, 3.885295173969059*^9}, {3.88538028847358*^9, 
   3.885380424562202*^9}, {3.8854507752358522`*^9, 3.885450817291069*^9}, {
   3.885450908438712*^9, 3.885450963267632*^9}, {3.8854512160156384`*^9, 
   3.885451260787283*^9}, {3.885456085051991*^9, 3.885456160635829*^9}, 
   3.885457606671516*^9, {3.885457953315969*^9, 
   3.885457958864456*^9}},ExpressionUUID->"6450b7e4-9cf3-4a95-bec2-\
5f5ba947ea83"],

Cell[TextData[{
 "The time derivatives above are then replaced by spatial derivatives using \
the definition of the PDE being solved, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", " ", "y"}], 
     RowBox[{"\[PartialD]", " ", "x"}]], " ", "=", " ", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[PartialD]", " ", "y"}], 
      RowBox[{"\[PartialD]", " ", "t"}]]}]}], TraditionalForm]],
  ExpressionUUID->"7b08ef7d-c48e-4b79-b35c-b08c17af36ec"],
 " :\[LineSeparator]  \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", " ", "t"}]], 
     RowBox[{"y", "[", 
      RowBox[{"x", ",", "t"}], "]"}]}], "    ", "=", "  ", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", " ", "x"}]], 
       RowBox[{"y", "[", 
        RowBox[{"x", ",", "t"}], "]"}]}]}], "   ", "\[TildeEqual]", "  ", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"y", "[", 
         RowBox[{
          RowBox[{"x", "+", 
           RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}], "-", 
        RowBox[{"y", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}]}], 
       RowBox[{"2", "\[Delta]", " ", "x"}]]}]}]}], TraditionalForm]],
  CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
  CellChangeTimes->{{3.8853758269198933`*^9, 3.88537583220271*^9}},
  ExpressionUUID->"3c25e8df-e95d-4103-803d-7a944d2dc47a"],
 ",\[LineSeparator]  \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["\[PartialD]", "2"], 
      RowBox[{"\[PartialD]", " ", 
       SuperscriptBox["t", "2"]}]], 
     RowBox[{"y", "[", 
      RowBox[{"x", ",", "t"}], "]"}]}], "  ", "=", "  ", 
    RowBox[{
     RowBox[{
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", " ", "t"}]], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", " ", "t"}]], 
       RowBox[{"y", "[", 
        RowBox[{"x", ",", "t"}], "]"}]}]}], "  ", "=", " ", 
     RowBox[{
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", " ", "t"}]], 
       RowBox[{"(", 
        RowBox[{"-", 
         RowBox[{
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", " ", "x"}]], 
          RowBox[{"y", "[", 
           RowBox[{"x", ",", "t"}], "]"}]}]}], ")"}]}], " ", "=", " ", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{
         FractionBox["\[PartialD]", 
          RowBox[{"\[PartialD]", " ", "x"}]], 
         RowBox[{
          FractionBox["\[PartialD]", 
           RowBox[{"\[PartialD]", " ", "t"}]], 
          RowBox[{"y", "[", 
           RowBox[{"x", ",", "t"}], "]"}]}]}]}], " ", "=", " ", 
       RowBox[{
        RowBox[{
         FractionBox[
          SuperscriptBox["\[PartialD]", "2"], 
          RowBox[{"\[PartialD]", " ", 
           SuperscriptBox["x", "2"]}]], 
         RowBox[{"y", "[", 
          RowBox[{"x", ",", "t"}], "]"}]}], "  ", "\[TildeEqual]", "  ", 
        FractionBox[
