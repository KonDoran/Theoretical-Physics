Cell[TextData[{
 "It is clear that we will also need a function capable of developing a \
single step in the time evolution of the solution, i.e., a function \
implementing the fundamental finite difference recurrence relation derived \
earlier:\[LineSeparator]  \[FilledSmallCircle]   ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "[", 
     RowBox[{
      SubscriptBox["x", "i"], ",", " ", 
      RowBox[{
       SubscriptBox["t", "j"], "+", 
       RowBox[{"\[Delta]", " ", "t"}]}]}], "]"}], "  ", "=", " ", 
    RowBox[{
     RowBox[{"y", "[", 
      RowBox[{
       SubscriptBox["x", "i"], ",", 
       SubscriptBox["t", "j"]}], "]"}], "  ", "-", " ", 
     RowBox[{
      FractionBox[
       RowBox[{"\[Delta]", " ", "t"}], 
       RowBox[{"\[Delta]", " ", "x"}]], 
      RowBox[{"(", " ", 
       RowBox[{
        RowBox[{"y", "[", 
         RowBox[{
          RowBox[{
           SubscriptBox["x", "i"], "+", 
           RowBox[{"\[Delta]", " ", "x"}]}], ",", 
          SubscriptBox["t", "j"]}], "]"}], "-", 
        RowBox[{"y", "[", 
         RowBox[{
          SubscriptBox["x", "i"], ",", 
          SubscriptBox["t", "j"]}], "]"}]}], " ", ")"}], "   ", 
      RowBox[{"\[ForAll]", "   ", 
       RowBox[{
        SubscriptBox["x", "min"], "<=", 
        SubscriptBox["x", "i"], "<=", 
        RowBox[{
         SubscriptBox["x", "max"], " ", "."}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"56bf2236-c31c-4906-b45a-354ed21d8e4c"],
 "\[LineSeparator]"
}], "Item1",
