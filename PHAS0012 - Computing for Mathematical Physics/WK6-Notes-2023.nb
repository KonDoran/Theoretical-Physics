 StyleBox["d",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " \[EscapeKey] , followed by, \[ControlKey]",
 StyleBox["+_",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 StyleBox[" ",
