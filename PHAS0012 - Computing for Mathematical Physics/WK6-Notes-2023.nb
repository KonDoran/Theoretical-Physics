Cell[TextData[{
 "Recall that in the original formulation of the finite difference scheme, at \
the start of this subsection, we replaced the spatial derivative according to\
\[LineSeparator]   \[FilledSmallCircle]  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", " ", "y"}], 
     RowBox[{"\[PartialD]", " ", "x"}]], "   ", "=", "   ", 
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{
       RowBox[{"\[Delta]", " ", "x"}], "\[Rule]", "0"}]], 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"y", "[", 
          RowBox[{
           RowBox[{"x", "+", 
            RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}], "-", 
         RowBox[{"y", "[", 
          RowBox[{"x", ",", "t"}], "]"}]}], 
        RowBox[{"\[Delta]", " ", "x"}]], " ", "\[LongRightArrow]", "  ", 
       FractionBox[
        RowBox[{
         RowBox[{"y", "[", 
          RowBox[{
           RowBox[{"x", "+", 
            RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}], "-", 
         RowBox[{"y", "[", 
          RowBox[{"x", ",", "t"}], "]"}]}], 
        RowBox[{"\[Delta]", " ", "x"}]]}], "  ", "."}]}]}], TraditionalForm]],
  ExpressionUUID->"8cdd1522-1c81-4925-aa8b-063be247bec7"],
 "\[LineSeparator]"
}], "Item1",
 CellChangeTimes->{{3.444031374666*^9, 3.4440313875369997`*^9}, {
