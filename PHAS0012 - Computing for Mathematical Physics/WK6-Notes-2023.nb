  RowBox[{"timeStepFnV2", "[", 
   RowBox[{"y_", ",", "\[Delta]x_", ",", "\[Delta]t_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"y", "-", 
