Cell[TextData[{
 "It is possible to increase the precision of the numerical solutions \
obtained via ",
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " by increasing the so-called ",
 StyleBox["WorkingPrecision",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " beyond its default, as demonstrated in the next code cell; wherein the \
latter exercise is repeated, verbatim, but with the ",
 StyleBox["WorkingPrecision",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " reset inside ",
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 "."
}], "Item1",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.8852073338808603`*^9, 3.885207341892274*^9}, {
   3.8852113420022697`*^9, 3.885211342092043*^9}, {3.885211692674616*^9, 
   3.885211834707271*^9}, 3.8852119669330587`*^9, 3.885212848390512*^9, {
   3.885212898584337*^9, 
   3.885212900923753*^9}},ExpressionUUID->"badf8bc7-0c8b-4a0e-a9f7-\
620bf02d2f1c"],

Cell[TextData[{
 "Note that for the increased ",
 StyleBox["WorkingPrecision",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " to work successfully one obviously has to take care that all other \
numerical inputs to ",
 StyleBox["NDSolve",
  FontFamily->"Courier New",
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 " have been specified with sufficiently ",
 StyleBox["high",
  FontSlant->"Italic"],
 " numerical precision."
}], "Item2",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.8852073338808603`*^9, 3.885207341892274*^9}, {
  3.8852113420022697`*^9, 3.885211342092043*^9}, {3.885211692674616*^9, 
  3.885212141681838*^9}},ExpressionUUID->"83814393-0efc-40f7-8647-\
82d78806c857"],

Cell["\<\
E.g. by adding a high precision specification to each one with the back-tick \
notation [`] discussed in the opening section of this notebook, or by \
entering them exactly, as exact numbers; i.e. converting decimal numbers to \
equivalent integers and fractions.\
\>", "Item2",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
 CellChangeTimes->{{3.8852073338808603`*^9, 3.885207341892274*^9}, {
  3.8852113420022697`*^9, 3.885211342092043*^9}, {3.885211692674616*^9, 
  3.885212193222466*^9}, {3.885212878403495*^9, 
  3.8852128799109783`*^9}},ExpressionUUID->"c7649024-9a57-4fdb-908a-\
f20db07e60d5"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Exact", " ", "same", " ", "code", " ", "as", " ", "before"}], 
    ",", " ", 
    RowBox[{"but", " ", "with", " ", "increased", " ", "precision"}]}], "   ",
    "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"in", " ", "the", " ", "numerical", " ", "solution"}], ",", " ", 
    RowBox[{
     RowBox[{"by", " ", "adding", " ", "WorkingPrecision"}], "->", "25."}]}], 
   " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"numericalSolutionReplacementRule", "=", "\[IndentingNewLine]", 
     RowBox[{"NDSolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"y", "''"}], "[", "x", "]"}], "+", 
           FractionBox["x", 
            RowBox[{"y", "[", "x", "]"}]]}], "==", "0"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"y", "'"}], "[", "0", "]"}], "==", "1"}], ",", 
         RowBox[{
          RowBox[{"y", "[", "0", "]"}], "\[Equal]", 
          FractionBox["1", "2"]}]}], "}"}], ",", 
       RowBox[{"y", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", 
         FractionBox["3", "2"]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"WorkingPrecision", "->", "25"}]}], "\[IndentingNewLine]", 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Exactly", " ", "as", " ", "before"}], " ", "..."}], " ", "*)"}],
    "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"numericalSolution", "[", "x_", "]"}], "=", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"y", "[", "x", "]"}], "/.", 
      "numericalSolutionReplacementRule"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Code", " ", "exactly", " ", "as", " ", "before"}], ",", " ", 
     RowBox[{
     "but", " ", "now", " ", "output", " ", "reveals", " ", "that", " ", 
      "the"}]}], "    ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "new", " ", "higher", " ", "precision", " ", "solution", " ", "satisfies",
      " ", "the", " ", "original"}], "       ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "differential", " ", "equation", " ", "a", " ", "couple", " ", "of", " ", 
     "orders", " ", "of", " ", "magnitude", " ", "more"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "precisely", " ", "than", " ", "in", " ", "the", " ", "first", " ", 
     RowBox[{"attempt", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Plot", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"numericalSolution", "''"}], "[", "x", "]"}], "+", 
      FractionBox["x", 
       RowBox[{"numericalSolution", "[", "x", "]"}]]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0", ",", 
       FractionBox["1", "5"]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"PlotRange", "\[Rule]", "All"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]], "Input",
 CellGroupingRules->{"GroupTogetherNestedGrouping", 15000.},
