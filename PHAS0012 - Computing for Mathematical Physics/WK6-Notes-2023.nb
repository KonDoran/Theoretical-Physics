Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Simple", " ", "first", " ", "order", " ", 
     RowBox[{"PDE", " ", ":", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "y"}], 
       RowBox[{"\[PartialD]", "x"}]]}]}], " ", "=", " ", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{"\[PartialD]", "y"}], 
      RowBox[{"\[PartialD]", "t"}]]}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"simplePDE", "=", "\[IndentingNewLine]", 
