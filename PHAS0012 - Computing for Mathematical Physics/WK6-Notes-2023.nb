Cell[TextData[{
 "We could, however, equally legitimately, chose to replace this derivative \
using ",
 StyleBox["backwards",
  FontSlant->"Italic"],
 " differences instead though, specifically, \[LineSeparator]   \
\[FilledSmallCircle]  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", " ", "y"}], 
     RowBox[{"\[PartialD]", " ", "x"}]], "   ", "=", "   ", 
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{
       RowBox[{"\[Delta]", " ", "x"}], "\[Rule]", "0"}]], 
     RowBox[{
      FractionBox[
       RowBox[{
        RowBox[{"y", "[", 
         RowBox[{"x", ",", "t"}], "]"}], "-", 
        RowBox[{"y", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}]}], 
       RowBox[{"\[Delta]", " ", "x"}]], " ", "\[LongRightArrow]", "  ", 
      FractionBox[
       RowBox[{
        RowBox[{"y", "[", 
         RowBox[{"x", ",", "t"}], "]"}], "-", 
        RowBox[{"y", "[", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"\[Delta]", " ", "x"}]}], ",", "t"}], "]"}]}], 
       RowBox[{"\[Delta]", " ", "x"}]], "  "}]}]}], TraditionalForm]],
  ExpressionUUID->"2b9fa036-b39f-4caa-92a1-bb4fd67499d7"],
 ".\[LineSeparator]"
