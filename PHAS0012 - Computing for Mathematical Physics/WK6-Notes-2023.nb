Cell[TextData[{
 "The system of equations here is ",
 StyleBox["coupled",
  FontSlant->"Italic"],
 " because ",
 "both differential equations depend on ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "(", "t", ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "dd76c569-a648-42e7-bf7e-44e9de214173"],
 " ",
 StyleBox["and",
  FontSlant->"Italic"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "2"], "(", "t", ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "72a3bd8e-cd2d-4952-8d7e-d6dbe03551cd"],
