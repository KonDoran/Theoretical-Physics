Cell[BoxData[
 RowBox[{
  RowBox[{"bisectionRootFinder", "[", 
   RowBox[{"f_", ",", "x1_", ",", "x2_", ",", "eps_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Which", "[", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Check", " ", "whether", " ", "the", " ", "function", " ", "has", " ", 
     "the", " ", "same", " ", "sign", " ", "at", " ", "x1", " ", "as", " ", 
     "at", " ", 
     RowBox[{"x2", "."}]}], "  ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"If", " ", "it", " ", 
     RowBox[{"doesn", "'"}], "t", " ", "then", " ", "we", " ", "assume", " ", 
     "it", " ", 
     RowBox[{"doesn", "'"}], "t", " ", "change", " ", "sign", " ", "at", " ", 
     "all", " ", "in"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"x1", "<", "x", "<", "x2"}], ",", " ", 
     RowBox[{
     "we", " ", "print", " ", "something", " ", "to", " ", "that", " ", 
      "effect"}], ",", " ", 
     RowBox[{
      RowBox[{"and", " ", "we", " ", "exit"}], " ", "..."}]}], "   ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Sign", "[", 
      RowBox[{"f", "[", "x1", "]"}], "]"}], "\[Equal]", 
     RowBox[{"Sign", "[", 
      RowBox[{"f", "[", "x2", "]"}], "]"}]}], ",", 
    "\"\<Starting values do not bracket root\>\"", ",", "\[IndentingNewLine]",
     "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "If", " ", "the", " ", "size", " ", "of", " ", "the", " ", "search", " ",
       "region", " ", "has", " ", "converged", " ", "to"}], "  ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"something", " ", "tiny", " ", "we", " ", "are", " ", 
      RowBox[{"done", ":", " ", 
       RowBox[{"we", " ", "return", " ", "the", " ", "midpoint"}]}]}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"of", " ", "the", " ", "interval", " ", "as", " ", "the", " ", 
      RowBox[{"root", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{"x1", "-", "x2"}], "]"}], "<", "eps"}], ",", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"x1", "+", "x2"}], ")"}]}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Otherwise", ",", " ", 
      RowBox[{
       RowBox[{
       "check", " ", "if", " ", "there", " ", "is", " ", "a", " ", "sign", 
        " ", "change", " ", "in", " ", "x1"}], " ", "<", " ", "x", " ", "<", 
       " ", 
       RowBox[{
        FractionBox["1", "2"], 
        RowBox[{"(", 
         RowBox[{"x1", "+", "x2"}], ")"}]}]}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"If", " ", "there", " ", "is"}], ",", " ", 
      RowBox[{
       RowBox[{
       "call", " ", "bisectionRootFinder", " ", "again", " ", "with", " ", 
        "x1"}], "=", 
       RowBox[{"x1", " ", "and"}]}]}], "      ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"x2", " ", "=", " ", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{"x1", "+", "x2"}], ")"}]}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Sign", "[", 
      RowBox[{"f", "[", "x1", "]"}], "]"}], "!=", 
     RowBox[{"Sign", "[", 
      RowBox[{"f", "[", 
       RowBox[{
        FractionBox["1", "2"], 
        RowBox[{"(", 
         RowBox[{"x1", "+", "x2"}], ")"}]}], "]"}], "]"}]}], ",", 
    RowBox[{"bisectionRootFinder", "[", 
     RowBox[{"f", ",", "x1", ",", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{"x1", "+", "x2"}], ")"}]}], ",", "eps"}], "]"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Otherwise", ",", " ", 
      RowBox[{
       RowBox[{
       "the", " ", "sign", " ", "change", " ", "must", " ", "be", " ", "in", 
