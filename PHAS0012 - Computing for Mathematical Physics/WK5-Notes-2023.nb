2659e28c8a69"]
}, Open  ]],

Cell[BoxData[
 RowBox[{"SphericalPlot3D", "[", "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Radius", " ", "coordinate", " ", "is", " ", "a", " ", "function", " ", 
    "of", " ", "\[Theta]", " ", "and", " ", "\[Phi]", " ", 
    RowBox[{"angles", "."}]}], "    ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Analogous", " ", "to", " ", "how"}], ",", " ", 
    RowBox[{"in", " ", "Plot3D"}], ",", " ", 
    RowBox[{
     RowBox[{"this", " ", "would", " ", "be", " ", "the", " ", "z"}], "-", 
     "axis"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"coordinate", ",", " ", 
    RowBox[{
    "as", " ", "a", " ", "function", " ", "of", " ", "x", " ", "and", " ", 
     RowBox[{"y", "."}]}]}], "                 ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Log", "[", "\[Theta]", "]"}], "+", 
    RowBox[{"Sin", "[", "\[CurlyPhi]", "]"}]}], ",", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Polar", " ", "angle"}], ",", " ", 
     RowBox[{"called", " ", "\[Theta]"}], ",", " ", 
     RowBox[{
     "but", " ", "it", " ", "is", " ", "just", " ", "a", " ", "label"}]}], 
    " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"\[Theta]", ",", "0", ",", 
     RowBox[{"2", "\[Pi]"}]}], "}"}], ",", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Azimuthal", " ", "angle"}], ",", " ", 
     RowBox[{"called", " ", "\[Phi]"}], ",", " ", 
     RowBox[{
     "but", " ", "it", " ", "is", " ", "just", " ", "a", " ", "label"}]}], 
    " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{"\[CurlyPhi]", ",", "0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}], ",", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Cosmetic", " ", "options"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Boxed", "\[Rule]", "False"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Axes", "\[Rule]", "False"}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotStyle", "\[Rule]", "Yellow"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Mesh", "\[Rule]", "None"}], ",", "\[IndentingNewLine]", 
   RowBox[{"PlotPoints", "\[Rule]", "30"}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
 CellChangeTimes->{{3.8842525330959997`*^9, 3.884252541486945*^9}, {
  3.884360138338883*^9, 3.8843602471952133`*^9}, {3.8843602800648413`*^9, 
  3.884360405611154*^9}},
 CellLabel->
  "2/8/23 14:52:11 \
In[6]:=",ExpressionUUID->"b0354a2c-8660-49fd-81df-7e658df683a9"]
}, Open  ]],

