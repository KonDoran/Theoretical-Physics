       RowBox[{"Length", "[", 
        RowBox[{"{", "a", "}"}], "]"}], "\[Equal]", "1"}], ",", 
      SuperscriptBox["a", "2"], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Otherwise", " ", "return", " ", "the", " ", "list", " ", 
        RowBox[{"{", "a", "}"}], " ", "with", " ", "every", " ", "element", 
        " ", "squared"}], " ", "*)"}], "\[IndentingNewLine]", "True", ",", 
      SuperscriptBox[
       RowBox[{"{", "a", "}"}], "2"]}], "\[IndentingNewLine]", "]"}]}], 
