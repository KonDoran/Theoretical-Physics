07b351583634"],

Cell["\<\
One can use such a construction to evaluate the Fibonacci series, in which \
each entry is the sum of the two previous numbers.\
\>", "Item1",
 CellChangeTimes->{
  3.884066872807951*^9, {3.884069817934598*^9, 
   3.884069844336267*^9}},ExpressionUUID->"78f8fb85-2bbe-4447-a74a-\
a227c765cc7c"]
}, Open  ]],

Cell[BoxData[
 RowBox[{
  RowBox[{"fib", "[", "n_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Which", "[", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
      "If", " ", "the", " ", "function", " ", "is", " ", "called", " ", 
       "with", " ", "n"}], "=", "1"}], ",", " ", 
     RowBox[{"return", " ", "1", " ", "and", " ", "exit"}]}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"n", "\[Equal]", "1"}], ",", "1", ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
       "If", " ", "the", " ", "function", " ", "is", " ", "called", " ", 
        "with", " ", "n"}], "=", "2"}], ",", " ", 
      RowBox[{"return", " ", "1", " ", "and", " ", "exit"}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"n", "\[Equal]", "2"}], ",", "1", ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "If", " ", "the", " ", "function", " ", "is", " ", "called", " ", "with",
       " ", "something", " ", "else", " ", "call", " ", "this"}], "  ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"function", " ", "again"}], ",", " ", 
      RowBox[{
       RowBox[{"with", " ", "n"}], "-", 
       RowBox[{"1", " ", "as", " ", "an", " ", "argument"}]}], ",", " ", 
      RowBox[{
       RowBox[{"and", " ", "with", " ", "n"}], "-", 
       RowBox[{"2", " ", "as"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"an", " ", "argument"}], ",", " ", 
      RowBox[{"adding", " ", "the", " ", "two", " ", "results"}]}], " ", 
     "*)"}], "\[IndentingNewLine]", "True", ",", 
    RowBox[{
     RowBox[{"fib", "[", 
      RowBox[{"n", "-", "1"}], "]"}], "+", 
     RowBox[{"fib", "[", 
      RowBox[{"n", "-", "2"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
