Cell[TextData[{
 "A comparison of the timings of these two different approaches, below, gives \
us an idea of just how much faster it can be to use a native ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " function; note that we repeat each summation below 50 times purely for \
illustrative purposes."
}], "Item1",
 CellChangeTimes->{{3.884067333829232*^9, 3.884067337991591*^9}, {
  3.884082525313037*^9, 
  3.88408274184651*^9}},ExpressionUUID->"8b27b522-40da-4e79-b8ff-\
e39ca9a5e259"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "Sum", " ", "square", " ", "roots", " ", "of", " ", "integers", " ", 
     "with", " ", "a", " ", "Do"}], "-", 
    RowBox[{"loop", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"SSSSLLLOOOWWW", "."}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Timing", "[", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"Do", " ", "the", " ", "summation", " ", 
      StyleBox["50",
       FontWeight->"Bold"], 
      StyleBox[" ",
       FontWeight->"Bold"], 
      StyleBox["times",
       FontWeight->"Bold"], " ", "with", " ", "a", " ", "Do"}], "-", "loop"}],
     " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Do", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"sum", "=", "0.0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "Do", " ", "each", " ", "of", " ", "those", " ", "50", " ", 
         "summations", " ", "with", " ", "the", " ", "Do"}], "-", 
        RowBox[{"loop", " ", 
         RowBox[{"below", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Do", "[", " ", 
       RowBox[{
        RowBox[{"sum", " ", "=", " ", 
         RowBox[{"sum", " ", "+", " ", 
          RowBox[{"N", "[", 
           RowBox[{"Sqrt", "[", "i", "]"}], "]"}]}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "500"}], "}"}]}], "\[IndentingNewLine]", 
       "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
