Cell[TextData[{
 StyleBox["The code in the cell directly below this one makes a plot with the \
following two elements.\[LineSeparator]\[LineSeparator]i) A green/yellow 3D \
surface, defined as function of ",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "0ce77f51-8d52-4258-a5b4-603a29a35cd4"],
 StyleBox[" and ",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox["y", TraditionalForm]],
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "bb852d49-d37f-4c06-bc4a-0a48a1661c91"],
 StyleBox[", with ",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox["z", TraditionalForm]],
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "4332a353-2897-432a-a522-a1781bf7df40"],
 StyleBox[" coordinates given by the function\[LineSeparator]\[LineSeparator]\
\[FilledSmallCircle] ",
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"surface", "[", 
     RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
    RowBox[{"BesselJ", "[", 
     RowBox[{"0", ",", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"x", "-", 
           FractionBox["1", "6"]}], ")"}], "2"], "+", 
        SuperscriptBox["y", "2"]}], ")"}]}], "]"}]}], ";"}]],
  CellChangeTimes->{{3.886210456074246*^9, 3.8862106137257223`*^9}, {
