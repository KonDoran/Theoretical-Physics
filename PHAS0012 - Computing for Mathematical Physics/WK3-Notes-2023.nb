Cell[BoxData[
 RowBox[{"logpowerrule", "=", 
  RowBox[{
   RowBox[{"Log", "[", 
    SuperscriptBox["x_", "n_"], "]"}], "\[Rule]", 
   RowBox[{"n", " ", 
    RowBox[{"Log", "[", "x", "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.882961441119326*^9, 3.882961443228615*^9}},
 CellLabel->
  "1/17/23 16:29:40 \
