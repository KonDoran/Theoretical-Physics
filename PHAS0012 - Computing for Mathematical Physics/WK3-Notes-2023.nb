Cell[TextData[{
 "We see that the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", " ", "b", " ", "a", " ", "b"}], TraditionalForm]],
  FontFamily->"Courier New",
  FontSize->16,
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.],ExpressionUUID->
  "de392e26-8cd7-4be1-a1a7-45ab0d2ad487"],
 " argument is not represented internally as ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{"a", "*", "b", "*", "a", "*", "b"}],
    FontFamily->"Courier New",
    FontSize->16,
    FontWeight->"Regular",
    FontColor->RGBColor[0., 0., 0.]], TraditionalForm]],ExpressionUUID->
  "5b39599b-1c3b-4390-a2bb-8458cb34e5b0"],
 ", but rather as explicit powers, ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     SuperscriptBox["a", "2"], 
     SuperscriptBox["b", "2"]}],
    FontFamily->"Courier New",
    FontSize->16,
    FontWeight->"Regular",
    FontColor->RGBColor[0., 0., 0.]], TraditionalForm]],ExpressionUUID->
  "4d2f43f2-bf3f-4680-b3dd-a8aa131987d1"],
 "."
}], "Item",
 CellChangeTimes->{{3.662393650619953*^9, 3.6623936510355597`*^9}, {
  3.8829599655771217`*^9, 3.88295997304248*^9}, {3.882961083259235*^9, 
  3.882961137705306*^9}, {3.88296120866551*^9, 3.8829613278106833`*^9}, {
  3.882961372754876*^9, 
  3.882961373891817*^9}},ExpressionUUID->"2c9826fc-dd18-4c2b-9787-\
722a871f1a96"],

Cell[TextData[{
 "The LHS of our ",
 StyleBox["logrule",
  FontFamily->"Courier New",
  FontSize->16,
  FontWeight->"Regular",
  FontColor->RGBColor[0., 0., 0.]],
 ", on the other hand, says nothing about powers, so cannot match to them, \
and hence the replacement is not applied to such objects."
}], "Item",
 CellChangeTimes->{{3.662393650619953*^9, 3.6623936510355597`*^9}, {
  3.8829599655771217`*^9, 3.88295997304248*^9}, {3.882961083259235*^9, 
  3.882961137705306*^9}, {3.88296120866551*^9, 
  3.8829613669066467`*^9}},ExpressionUUID->"6ec7cc71-56e0-4beb-bd2a-\
f2921ebb1088"],

Cell[CellGroupData[{

Cell["\<\
To get around this issue, we can define a second, simple, supplementary \
replacement rule as follows.\
\>", "Item",
 CellChangeTimes->{{3.662393650619953*^9, 3.6623936510355597`*^9}, {
  3.8829599655771217`*^9, 3.8829599927538357`*^9}, {3.882961412386312*^9, 
  3.882961433310132*^9}},ExpressionUUID->"4e29401d-4847-422c-8fac-\
682c68ed10bb"],

