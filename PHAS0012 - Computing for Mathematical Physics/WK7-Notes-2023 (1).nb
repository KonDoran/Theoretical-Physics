Cell[TextData[{
 StyleBox["FixedPoint",
  FontFamily->"Courier New"],
 " and ",
 StyleBox["FixedPointList",
  FontFamily->"Courier New"],
 " are, in essence, special versions of ",
 StyleBox["NestWhile",
  FontFamily->"Courier New"],
 " and ",
 StyleBox["NestWhileList",
  FontFamily->"Courier New"],
 ".\[LineSeparator]"
}], "Item1",
 CellChangeTimes->{{3.8852378099210987`*^9, 3.885237809994722*^9}, {
